<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

<form id="gerar-boleto" >
  <h2>Pagar com cartão</h2><br>
  <label for="cpf">CPF:</label>
  <input type="text" id="cpf" name="cpf"><br><br>

  <label for="cliente">Cliente:</label>
  <input type="text" id="cliente" name="cliente"><br><br>

  <label for="preco">Preço:</label>
  <input type="text" id="preco" name="preco"><br><br>

  <input type="submit" value="Submit">
</form>

<script>
  $(document).ready(function(){
    $( "#gerar-boleto" ).submit(function( event ) {
      event.preventDefault();

      var $form = $( this );
      var cpf = $form.find( "input[name='cpf']" ).val();
      var cliente = $form.find( "input[name='cliente']" ).val();
      var preco = $form.find( "input[name='preco']" ).val();

      $.post( "/gerar-boleto", { cpf, cliente, preco }, () => {} ,'json');
    });
  });
</script>