{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "esnext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "removeComments": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "sourceMap": true,
  },
  "include": ["src"],
  "exclude": []
}